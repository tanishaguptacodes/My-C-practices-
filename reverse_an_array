#include <iostream>
using namespace std;

int main() {
    int  n;
    cout << "How many elements you want to insert in an array: ";
    cin >> n;
    int a[n];
    cout << "Enter numbers for the array: ";
    for (int i = 0; i < n; i++) {
        cin >> a[i];
    }
    cout<<"Reverse an array:";
    for (int i = n-1; i>=0; i--) {
        cout<< a[i]<<" ";
    }
    cout<<endl;
    
    return 0;
}