#include <iostream>
using namespace std;

int swapvalues(int &x, int &y) {
    int t=x;
    x=y;
    y=t;
    return x,y;
}

int main() {
    int a,b;
    cout<<"Enter number A:";
    cin>>a;
    cout<<"Enter number B:";
    cin>>b;
    cout<<"Before swaping A is:"<<a<<endl<<"B is:"<<b<<endl;
    swapvalues(a,b);
    cout<<"After swaping A is:"<<a<<endl<<"B is:"<<b;
    return 0;
}