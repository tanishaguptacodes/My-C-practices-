#include <iostream>
using namespace std;

int main() {
    bool found = false;
    int p, n;

    cout << "How many elements you want to insert in an array: ";
    cin >> n;
    int a[n];
    cout << "Enter numbers for the array: ";
    for (int i = 0; i < n; i++) {
        cin >> a[i];
    }

    cout << "Enter element you want to find: ";
    cin >> p;

    for (int i = 0; i < n; i++) {
        if (a[i] == p) {
            found = true;
            break;  
        }
    }

    if (found) {
        cout << "Element is in the array" << endl;
    } else {
        cout << "Element is not in the array" << endl;
    }

    return 0;
}