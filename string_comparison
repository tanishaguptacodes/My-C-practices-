#include<iostream>
#include<string>
using namespace std;
int main(){
    string r,s="";
    cout<<"Enter a first string:";
    cin>>s;
    cout<<"Enter a second string:";
    cin>>r;
    if(s==r){
        cout<<"both the strings are same";
    }
    else{
        cout<<"strings are not same ";
    }
    return 0;
}