#include<iostream>
#include<string>
using namespace std;
int main(){
    string r,s="";
    cout<<"Enter a sentence:";
    getline(cin,s);
    for(int i=0;i<=s.length();i++){
        if(s[i]!=' '){
            r+=s[i];
            
        }
        
    }
    cout<<"After removing spaces:"<<r;

return 0;

}

