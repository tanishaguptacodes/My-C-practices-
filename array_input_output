#include<iostream>
using namespace std;
int main(){
    int n;
    cout<<"How many elements you want to insert in an array:";
    cin>>n;
    int a[n];
    cout<<"Enter numbers without commas(,) for array:";
    for(int i=0;i<n;i++){
        cin>>a[i];
    }
    cout<<"The elements you have entered for array are:"<<endl;
    for(int i=0;i<n;i++){
        cout<<a[i]<<endl;
    }
    return 0;
}